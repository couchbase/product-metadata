{
  "couchbase-lite-c": {
    "max_build_time": 120,
    "package": {
      "couchbase-lite-c": {
        "release": {
          "3.0.0": {
            "edition": [
              "community",
              "enterprise"
            ],
            "platform": {
              "android": {
                "ext": "zip"
              },
              "debian10-x86_64": {
                "ext": "tar.gz"
              },
              "debian9-x86_64": {
                "ext": "tar.gz"
              },
              "ios": {
                "ext": "zip"
              },
              "macosx": {
                "ext": "zip"
              },
              "raspbian9": {
                "ext": "tar.gz"
              },
              "raspios10-arm64": {
                "ext": "tar.gz"
              },
              "raspios10-armhf": {
                "ext": "tar.gz"
              },
              "ubuntu20.04-armhf": {
                "ext": "tar.gz"
              },
              "ubuntu20.04-x86_64": {
                "ext": "tar.gz"
              },
              "windows-x86_64": {
                "ext": "zip"
              }
            },
            "template": "{package}-{edition}-{VERSION}-{BLD_NUM}-{platform}.{ext}"
          }
        }
      },
      "libcblite": {
        "release": {
          "3.0.0": {
            "edition": [
              "community",
              "enterprise"
            ],
