            ],
            "platform": {
              "debian10_amd64": {},
              "debian10_arm64": {},
              "debian10_armhf": {},
              "debian9_amd64": {},
              "debian9_armhf": {},
              "raspbian9_armhf": {},
              "raspios10_arm64": {},
              "raspios10_armhf": {},
              "ubuntu20.04_amd64": {},
              "ubuntu20.04_armhf": {}
            },
            "template": "{package}{dev}-{edition}_{VERSION}-{BLD_NUM}-{platform}.deb"
          }
        }
      }
    }
  }
}
